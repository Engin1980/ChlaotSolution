<UserControl x:Class="AffinityModule.CtrRun"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AffinityModule"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
  <DataGrid ItemsSource="{Binding ProcessInfos}" AutoGenerateColumns="False">
    <DataGrid.Columns>
      <DataGridTextColumn Header="Id" Binding="{Binding ID}"/>
      <DataGridTextColumn Header="Name" Binding="{Binding Name}"/>
      <DataGridTextColumn Header="Window Title" Binding="{Binding WindowTitle}"/>
      <DataGridTextColumn Header="Applied Rule" Binding="{Binding RuleTitle}"/>
    </DataGrid.Columns>
  </DataGrid>
</UserControl>
