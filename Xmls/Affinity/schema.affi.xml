<?xml version="1.0" encoding="utf-8" ?>
<affinityRuleBase
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://tempuri.org/Schema.xsd"
  xsi:noNamespaceSchemaLocation="file://./Xsds/AffinitySchema.xsd">
  <metaInfo>
    <label>Simple Affinity Schema</label>
    <author>Marek Vajgl</author>
    <description>
      FS2020 uses mostly 4-5, only sometimes about 6 threads. 
      This division is created for =&gt;12 cores CPU.
    </description>
  </metaInfo>
  <rule title="Mozilla" regex="Firefox" roll="0-4" />
  <rule title="Idle" regex="Idle" roll="" />
  <rule title="Other" regex=".+" roll="6-64" />
</affinityRuleBase>
